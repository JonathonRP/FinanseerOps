{
	"name": "personalfinancedashboard",
	"version": "0.0.1",
	"scripts": {
		"db:seed": "dotenv -e .env.local -- pnpx prisma db seed",
		"db:push": "dotenv -e .env.local -- pnpx prisma db push",
		"prisma:generate": "dotenv -e .env.local -- pnpx prisma generate",
		"dev": "concurrently 'vite dev' 'quirrel'",
		"sync": "svelte-kit sync",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"build": "vite build",
		"debug": "vite dev",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"prisma": {
		"seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
	},
	"dependencies": {
		"@auth/core": "^0.4.0",
		"@auth/sveltekit": "^0.2.1",
		"@js-temporal/polyfill": "^0.4.3",
		"@next-auth/prisma-adapter": "^1.0.5",
		"@prisma/client": "^4.10.1",
		"@trpc/client": "^10.12.0",
		"@trpc/server": "^10.12.0",
		"iconify-icon": "^1.0.6",
		"nodemailer": "^6.9.1",
		"quirrel": "^1.13.2",
		"rxjs": "^7.8.0",
		"superjson": "^1.12.2",
		"sweetalert2": "^11.7.2",
		"trpc-sveltekit": "^3.3.1",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@iconify-icons/fa6-solid": "^1.2.11",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^1.0.3",
		"@sveltejs/kit": "^1.8.2",
		"@tailwindcss/forms": "^0.5.3",
		"@types/cookie": "^0.5.1",
		"@types/node": "^18.14.0",
		"@types/nodemailer": "^6.4.7",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"cookie": "^0.5.0",
		"date-fns": "^2.29.3",
		"dotenv": "^16.0.3",
		"dotenv-cli": "^7.0.0",
		"eslint": "^8.34.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.19.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "2.8.3",
		"prettier-plugin-prisma": "^4.10.0",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.3",
		"prisma": "^4.10.1",
		"prisma-zod-generator": "^0.8.11",
		"sass": "^1.58.3",
		"svelte": "^3.55.1",
		"svelte-add": "2022.12.0-9.0",
		"svelte-check": "^3.0.3",
		"svelte-french-toast": "^1.0.3",
		"svelte-motion": "^0.11.5",
		"svelte-transition-classes": "^0.1.1",
		"tailwindcss": "^3.2.7",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.3"
	},
	"type": "module"
}
